<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_detail_case"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:focusable="true"
    android:fitsSystemWindows="true"
    android:focusableInTouchMode="true"
    android:descendantFocusability="blocksDescendants"
    tools:context=".Views.Activities.DetailCaseActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layoutDirection="rtl"
        android:orientation="vertical"
        android:background="@color/White">

        <include layout="@layout/component_toolbar" />

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/_1sdp"
            android:background="@color/Quartz" />

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <FrameLayout
                android:id="@+id/activity_detail_case_mainLayout"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:visibility="gone">

                <androidx.core.widget.NestedScrollView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scrollbarSize="0dp"
                    android:overScrollMode="never">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <androidx.cardview.widget.CardView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_16sdp"
                            android:layout_marginBottom="@dimen/_8sdp"
                            android:layout_marginLeft="@dimen/_16sdp"
                            android:layout_marginRight="@dimen/_16sdp"
                            app:cardBackgroundColor="@color/Snow"
                            app:cardCornerRadius="@dimen/_16sdp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_margin="@dimen/_12sdp"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/activity_detail_case_name_textView"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:gravity="right|center"
                                    android:text="@string/DetailCaseName"
                                    android:textSize="@dimen/_10ssp"
                                    android:textStyle="bold"
                                    android:textColor="@color/Nero"
                                    android:textAlignment="gravity" />

                                <View
                                    android:layout_width="match_parent"
                                    android:layout_height="@dimen/_1sdp"
                                    android:layout_marginTop="@dimen/_8sdp"
                                    android:layout_marginBottom="@dimen/_12sdp"
                                    android:background="@color/Quartz" />

                                <RelativeLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content">

                                    <FrameLayout
                                        android:id="@+id/activity_detail_case_room_avatar_frameLayout"
                                        android:layout_width="@dimen/_44sdp"
                                        android:layout_height="@dimen/_44sdp"
                                        android:layout_alignParentTop="true"
                                        android:layout_alignParentRight="true">

                                        <de.hdodenhof.circleimageview.CircleImageView
                                            android:id="@+id/activity_detail_case_room_avatar_imageView"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent" />

                                        <TextView
                                            android:id="@+id/activity_detail_case_room_subtitle_textView"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:gravity="center"
                                            android:visibility="gone"
                                            android:textSize="@dimen/_12ssp"
                                            android:textStyle="bold"
                                            android:textColor="@color/Grey"
                                            android:textAlignment="gravity"/>

                                    </FrameLayout>

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_toLeftOf="@id/activity_detail_case_room_avatar_frameLayout"
                                        android:layout_marginRight="@dimen/_12sdp"
                                        android:layout_centerVertical="true"
                                        android:orientation="vertical">

                                        <TextView
                                            android:id="@+id/activity_detail_case_room_title_textView"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:gravity="right"
                                            android:textSize="@dimen/_12ssp"
                                            android:textStyle="bold"
                                            android:textColor="@color/Grey"
                                            android:textAlignment="gravity"/>

                                        <TextView
                                            android:id="@+id/activity_detail_case_room_type_textView"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_marginTop="@dimen/_minus4sdp"
                                            android:gravity="right"
                                            android:textSize="@dimen/_9ssp"
                                            android:textStyle="bold"
                                            android:textColor="@color/Mischka"
                                            android:textAlignment="gravity" />

                                    </LinearLayout>

                                </RelativeLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_12sdp"
                                    android:layout_marginBottom="@dimen/_12sdp"
                                    android:paddingTop="@dimen/_6sdp"
                                    android:paddingBottom="@dimen/_6sdp"
                                    android:paddingLeft="@dimen/_8sdp"
                                    android:paddingRight="@dimen/_8sdp"
                                    android:orientation="vertical"
                                    android:background="@drawable/draw_8sdp_solid_primary5p">

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginBottom="@dimen/_2sdp"
                                        android:gravity="right"
                                        android:text="@string/DetailCaseComplaint"
                                        android:textSize="@dimen/_9ssp"
                                        android:textStyle="bold"
                                        android:textColor="@color/PrimaryDark"
                                        android:textAlignment="gravity"/>

                                    <TextView
                                        android:id="@+id/activity_detail_case_complaint_textView"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:gravity="right"
                                        android:textSize="@dimen/_9ssp"
                                        android:textStyle="normal"
                                        android:textColor="@color/Grey"
                                        android:textAlignment="gravity" />

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:paddingLeft="@dimen/_2sdp"
                                    android:paddingRight="@dimen/_2sdp"
                                    android:orientation="vertical">

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginBottom="@dimen/_4sdp"
                                        android:visibility="gone"
                                        android:orientation="horizontal"
                                        android:layoutDirection="rtl">

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:gravity="right"
                                            android:text="@string/DetailCaseSessionsCount"
                                            android:textSize="@dimen/_9ssp"
                                            android:textStyle="bold"
                                            android:textColor="@color/Mischka"
                                            android:textAlignment="gravity" />

                                        <TextView
                                            android:id="@+id/activity_detail_case_sessions_count_textView"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:gravity="left"
                                            android:textSize="@dimen/_9ssp"
                                            android:textStyle="bold"
                                            android:textColor="@color/Grey"
                                            android:textAlignment="gravity" />

                                    </LinearLayout>

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginBottom="@dimen/_4sdp"
                                        android:visibility="gone"
                                        android:orientation="horizontal"
                                        android:layoutDirection="rtl">

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:gravity="right"
                                            android:text="@string/DetailCaseTestsCount"
                                            android:textSize="@dimen/_9ssp"
                                            android:textStyle="bold"
                                            android:textColor="@color/Mischka"
                                            android:textAlignment="gravity" />

                                        <TextView
                                            android:id="@+id/activity_detail_case_tests_count_textView"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:gravity="left"
                                            android:textSize="@dimen/_9ssp"
                                            android:textStyle="bold"
                                            android:textColor="@color/Grey"
                                            android:textAlignment="gravity" />

                                    </LinearLayout>

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginBottom="@dimen/_4sdp"
                                        android:visibility="gone"
                                        android:orientation="horizontal"
                                        android:layoutDirection="rtl">

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:gravity="right"
                                            android:text="@string/DetailCaseLastSessionDate"
                                            android:textSize="@dimen/_9ssp"
                                            android:textStyle="bold"
                                            android:textColor="@color/Mischka"
                                            android:textAlignment="gravity" />

                                        <TextView
                                            android:id="@+id/activity_detail_case_last_session_date_textView"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:gravity="left"
                                            android:textSize="@dimen/_9ssp"
                                            android:textStyle="bold"
                                            android:textColor="@color/Grey"
                                            android:textAlignment="gravity" />

                                    </LinearLayout>

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:orientation="horizontal"
                                        android:layoutDirection="rtl">

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:gravity="right"
                                            android:text="@string/DetailCaseCreatedCaseDate"
                                            android:textSize="@dimen/_9ssp"
                                            android:textStyle="bold"
                                            android:textColor="@color/Mischka"
                                            android:textAlignment="gravity" />

                                        <TextView
                                            android:id="@+id/activity_detail_case_created_case_date_textView"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:gravity="left"
                                            android:textSize="@dimen/_9ssp"
                                            android:textStyle="bold"
                                            android:textColor="@color/Grey"
                                            android:textAlignment="gravity" />

                                    </LinearLayout>

                                </LinearLayout>

                            </LinearLayout>

                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_8sdp"
                            android:layout_marginLeft="@dimen/_16sdp"
                            android:layout_marginRight="@dimen/_16sdp"
                            app:cardBackgroundColor="@color/Snow"
                            app:cardCornerRadius="@dimen/_16sdp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_margin="@dimen/_12sdp"
                                android:orientation="vertical">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layoutDirection="ltr"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:id="@+id/activity_detail_case_add_reference_textView"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center|left"
                                        android:paddingTop="@dimen/_3sdp"
                                        android:paddingBottom="@dimen/_3sdp"
                                        android:paddingLeft="@dimen/_8sdp"
                                        android:paddingRight="@dimen/_8sdp"
                                        android:gravity="center"
                                        android:text="@string/DetailCaseAddReference"
                                        android:textSize="@dimen/_8ssp"
                                        android:textStyle="bold"
                                        android:textColor="@color/White"
                                        android:textAlignment="gravity"
                                        android:background="@drawable/draw_8sdp_solid_primary"/>

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:gravity="right|center"
                                        android:text="@string/DetailCaseReferences"
                                        android:textSize="@dimen/_10ssp"
                                        android:textStyle="bold"
                                        android:textColor="@color/Nero"
                                        android:textAlignment="gravity" />

                                </LinearLayout>

                                <View
                                    android:layout_width="match_parent"
                                    android:layout_height="@dimen/_1sdp"
                                    android:layout_marginTop="@dimen/_8sdp"
                                    android:layout_marginBottom="@dimen/_12sdp"
                                    android:background="@color/Quartz" />

                                <androidx.recyclerview.widget.RecyclerView
                                    android:id="@+id/activity_detail_case_references_recyclerView"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:overScrollMode="never" />

                                <TextView
                                    android:id="@+id/activity_detail_case_references_empty_textView"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:visibility="gone"
                                    android:gravity="center"
                                    android:text="@string/DetailCaseEmptyReference"
                                    android:textSize="@dimen/_8ssp"
                                    android:textStyle="bold"
                                    android:textColor="@color/Mischka"
                                    android:textAlignment="gravity" />

                            </LinearLayout>

                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_8sdp"
                            android:layout_marginLeft="@dimen/_16sdp"
                            android:layout_marginRight="@dimen/_16sdp"
                            app:cardBackgroundColor="@color/Snow"
                            app:cardCornerRadius="@dimen/_16sdp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_margin="@dimen/_12sdp"
                                android:orientation="vertical">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layoutDirection="ltr"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:id="@+id/activity_detail_case_add_session_textView"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center|left"
                                        android:paddingTop="@dimen/_3sdp"
                                        android:paddingBottom="@dimen/_3sdp"
                                        android:paddingLeft="@dimen/_8sdp"
                                        android:paddingRight="@dimen/_8sdp"
                                        android:gravity="center"
                                        android:text="@string/DetailCaseAddSession"
                                        android:textSize="@dimen/_8ssp"
                                        android:textStyle="bold"
                                        android:textColor="@color/White"
                                        android:textAlignment="gravity"
                                        android:background="@drawable/draw_8sdp_solid_primary"/>

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:gravity="right|center"
                                        android:text="@string/DetailCaseSessions"
                                        android:textSize="@dimen/_10ssp"
                                        android:textStyle="bold"
                                        android:textColor="@color/Nero"
                                        android:textAlignment="gravity" />

                                </LinearLayout>

                                <View
                                    android:layout_width="match_parent"
                                    android:layout_height="@dimen/_1sdp"
                                    android:layout_marginTop="@dimen/_8sdp"
                                    android:layout_marginBottom="@dimen/_12sdp"
                                    android:background="@color/Quartz" />

                                <LinearLayout
                                    android:id="@+id/activity_detail_case_sessions_hint_linearLayout"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="@dimen/_8sdp"
                                    android:layoutDirection="rtl"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_weight="0.95"
                                        android:gravity="bottom|right"
                                        android:text="@string/DetailCaseSessionId"
                                        android:textSize="@dimen/_9ssp"
                                        android:textStyle="bold"
                                        android:textColor="@color/Mischka"
                                        android:textAlignment="gravity" />

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:layout_marginRight="@dimen/_4sdp"
                                        android:layout_marginLeft="@dimen/_2sdp"
                                        android:gravity="bottom|right"
                                        android:text="@string/DetailCaseSessionStartedAt"
                                        android:textSize="@dimen/_9ssp"
                                        android:textStyle="bold"
                                        android:textColor="@color/Mischka"
                                        android:textAlignment="gravity" />

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_marginRight="@dimen/_2sdp"
                                        android:layout_marginLeft="@dimen/_4sdp"
                                        android:layout_weight="1.05"
                                        android:gravity="bottom|right"
                                        android:text="@string/DetailCaseSessionDuration"
                                        android:textSize="@dimen/_9ssp"
                                        android:textStyle="bold"
                                        android:textColor="@color/Mischka"
                                        android:textAlignment="gravity" />

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:gravity="bottom|right"
                                        android:text="@string/DetailCaseSessionStatus"
                                        android:textSize="@dimen/_9ssp"
                                        android:textStyle="bold"
                                        android:textColor="@color/Mischka"
                                        android:textAlignment="gravity" />

                                    <View
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1" />

                                </LinearLayout>

                                <androidx.recyclerview.widget.RecyclerView
                                    android:id="@+id/activity_detail_case_sessions_recyclerView"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:overScrollMode="never" />

                                <TextView
                                    android:id="@+id/activity_detail_case_sessions_empty_textView"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:visibility="gone"
                                    android:gravity="center"
                                    android:text="@string/DetailCaseEmptySession"
                                    android:textSize="@dimen/_8ssp"
                                    android:textStyle="bold"
                                    android:textColor="@color/Mischka"
                                    android:textAlignment="gravity" />

                            </LinearLayout>

                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_16sdp"
                            android:layout_marginLeft="@dimen/_16sdp"
                            android:layout_marginRight="@dimen/_16sdp"
                            app:cardBackgroundColor="@color/Snow"
                            app:cardCornerRadius="@dimen/_16sdp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_margin="@dimen/_12sdp"
                                android:orientation="vertical">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layoutDirection="ltr"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:id="@+id/activity_detail_case_create_sample_textView"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center|left"
                                        android:paddingTop="@dimen/_3sdp"
                                        android:paddingBottom="@dimen/_3sdp"
                                        android:paddingLeft="@dimen/_8sdp"
                                        android:paddingRight="@dimen/_8sdp"
                                        android:gravity="center"
                                        android:text="@string/DetailCaseCreateSample"
                                        android:textSize="@dimen/_8ssp"
                                        android:textStyle="bold"
                                        android:textColor="@color/White"
                                        android:textAlignment="gravity"
                                        android:background="@drawable/draw_8sdp_solid_primary"/>

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:gravity="right|center"
                                        android:text="@string/DetailCaseSamples"
                                        android:textSize="@dimen/_10ssp"
                                        android:textStyle="bold"
                                        android:textColor="@color/Nero"
                                        android:textAlignment="gravity" />

                                </LinearLayout>

                                <View
                                    android:layout_width="match_parent"
                                    android:layout_height="@dimen/_1sdp"
                                    android:layout_marginTop="@dimen/_8sdp"
                                    android:layout_marginBottom="@dimen/_12sdp"
                                    android:background="@color/Quartz" />

                                <LinearLayout
                                    android:id="@+id/activity_detail_case_samples_hint_linearLayout"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="@dimen/_8sdp"
                                    android:layoutDirection="rtl"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:gravity="top|right"
                                        android:text="@string/DetailCaseSampleTitle"
                                        android:textSize="@dimen/_9ssp"
                                        android:textStyle="bold"
                                        android:textColor="@color/Mischka"
                                        android:textAlignment="gravity" />

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_marginRight="@dimen/_4sdp"
                                        android:layout_marginLeft="@dimen/_2sdp"
                                        android:layout_weight="1"
                                        android:gravity="top|right"
                                        android:text="@string/DetailCaseSampleStatus"
                                        android:textSize="@dimen/_9ssp"
                                        android:textStyle="bold"
                                        android:textColor="@color/Mischka"
                                        android:textAlignment="gravity" />

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_marginRight="@dimen/_2sdp"
                                        android:layout_marginLeft="@dimen/_4sdp"
                                        android:layout_weight="0.9"
                                        android:gravity="top|right"
                                        android:text="@string/DetailCaseSampleSessionId"
                                        android:textSize="@dimen/_9ssp"
                                        android:textStyle="bold"
                                        android:textColor="@color/Mischka"
                                        android:textAlignment="gravity" />

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:gravity="top|right"
                                        android:text="@string/DetailCaseSampleReference"
                                        android:textSize="@dimen/_9ssp"
                                        android:textStyle="bold"
                                        android:textColor="@color/Mischka"
                                        android:textAlignment="gravity" />

                                    <View
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1.1" />

                                </LinearLayout>

                                <androidx.recyclerview.widget.RecyclerView
                                    android:id="@+id/activity_detail_case_samples_recyclerView"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:overScrollMode="never" />

                                <TextView
                                    android:id="@+id/activity_detail_case_samples_empty_textView"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:visibility="gone"
                                    android:gravity="center"
                                    android:text="@string/DetailCaseEmptySample"
                                    android:textSize="@dimen/_8ssp"
                                    android:textStyle="bold"
                                    android:textColor="@color/Mischka"
                                    android:textAlignment="gravity" />

                            </LinearLayout>

                        </androidx.cardview.widget.CardView>

                    </LinearLayout>

                </androidx.core.widget.NestedScrollView>

            </FrameLayout>

            <include
                layout="@layout/layout_info"
                android:visibility="gone" />

            <include
                layout="@layout/layout_loading"
                android:visibility="visible" />

        </FrameLayout>

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/_1sdp"
        android:layout_gravity="bottom"
        android:background="@color/Quartz" />

</androidx.coordinatorlayout.widget.CoordinatorLayout>